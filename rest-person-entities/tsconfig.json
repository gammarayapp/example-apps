{
  "compilerOptions": {
    "module": "ES2020",
    "esModuleInterop": true,
    "target": "ES2020",
    "moduleResolution": "Node",
    "removeComments": true,
    "outDir": "./dist",
    "rootDir": "./",
    "strict": true,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  }
}
